## Init settings.
# Parameter list to optimize.
param_list=( ADX5_SignalMethod )
## Backtest settings.
ini_set ^TestPeriod M5
## EA settings.
input_set ^CloseOrderAfterXHours 72
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Enable strategy.
input_set ^ADX5_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test ADX5_SignalMethod.
input_set ^ADX5_SignalMethod,F 1
input_set ^ADX5_SignalMethod,1 0
input_set ^ADX5_SignalMethod,2 1
input_set ^ADX5_SignalMethod,3 15

# $ETA:$
