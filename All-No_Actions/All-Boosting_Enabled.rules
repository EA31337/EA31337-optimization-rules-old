## Init settings.
# Parameter list to optimize.
param_list=( Boosting_Enabled )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable account conditions.
ea_actions_enabled 0

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Boosting_Enabled.
input_set Boosting_Enabled,F 1
input_set Boosting_Enabled,1 0
input_set Boosting_Enabled,2 1
input_set Boosting_Enabled,3 1

