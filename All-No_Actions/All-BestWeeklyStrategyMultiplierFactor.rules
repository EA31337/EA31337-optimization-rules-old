## Init settings.
# Parameter list to optimize.
param_list=( BestWeeklyStrategyMultiplierFactor )
# Disable all optimization settings.
ea_optimize_all_set 0
# Enable boosting.
input_set Boosting_Enabled 1
# Disable account conditions.
ea_actions_enabled 0

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test BestWeeklyStrategyMultiplierFactor.
input_set BestWeeklyStrategyMultiplierFactor,F 1
input_set BestWeeklyStrategyMultiplierFactor,1 $( bc <<< "$(input_get BestWeeklyStrategyMultiplierFactor) - 0.2" )
input_set BestWeeklyStrategyMultiplierFactor,2 0.1
input_set BestWeeklyStrategyMultiplierFactor,3 $( bc <<< "$(input_get BestWeeklyStrategyMultiplierFactor) + 0.2" )

